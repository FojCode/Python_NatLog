# Code to calculate and print a table of powers
print("a | b | result")
print("---------")
for a in range(1, 11):
        for b in range(1, 5):
                result = power(a, b)
                print(f"{a} | {b} | {result}")

# Calculate and print the result for a specific base and exponent
base = int(input("Enter the base: "))
exponent = int(input("Enter the exponent: "))
result = power(base, exponent)
print(f"{base} raised to the power of {exponent} is {result}")
import math # calculating ln
def natural_logarithm(x):
        return math.log(x)
x = 10 # example
result = natural_logarithm(x)
print(f"The natural logarithm of {x} is {result}")
x = 2.71828 # x is the special value e
result = natural_logarithm(x)
print(f"The natural logarithm of {x} is {result}")

import numpy as np # using numpy to calculate ln
def natural_logarithm(x):
        return np.log(x)
x = 10 # example 
result = natural_logarithm(x)
print(f"The natural logarithm of {x} is {result}")


x = 2.71828
result = natural_logarithm(x)
print(f"The natural logarithm of {x} is {result}")
